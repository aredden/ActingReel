<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <content-default
                        jcr:description="Policy for editable layout containers"
                        jcr:lastModified="{Date}2019-11-15T16:46:53.328-05:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Acting Reel Content Policy"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/libs/wcm/foundation/components/responsivegrid,/apps/actingreel/components/content/breadcrumb,/apps/actingreel/components/content/image,/apps/actingreel/components/content/list,/apps/actingreel/components/content/text,/apps/actingreel/components/content/title,/apps/actingreel/components/content/topnav]"
                        policyResourceType="wcm/foundation/components/responsivegrid">
                        <cq:authoring jcr:primaryType="nt:unstructured">
                            <assetToComponentMapping jcr:primaryType="nt:unstructured">
                                <image
                                    jcr:primaryType="nt:unstructured"
                                    assetGroup="media"
                                    assetMimetype="image/*"
                                    droptarget="image"
                                    resourceType="actingreel/components/content/image"
                                    type="Images"/>
                            </assetToComponentMapping>
                        </cq:authoring>
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            mergeList="{Boolean}true"/>
                    </content-default>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
    <actingreel jcr:primaryType="nt:unstructured">
        <components jcr:primaryType="nt:unstructured">
            <form jcr:primaryType="nt:unstructured">
                <container jcr:primaryType="nt:unstructured">
                    <form-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="actingreel Form"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/apps/actingreel/components/form/button,/apps/actingreel/components/form/hidden,/apps/actingreel/components/form/options,/apps/actingreel/components/form/text]">
                        <jcr:content jcr:primaryType="nt:unstructured"/>
                    </form-container>
                </container>
            </form>
            <content jcr:primaryType="nt:unstructured">
                <topnav jcr:primaryType="nt:unstructured"/>
                <text jcr:primaryType="nt:unstructured"/>
                <image jcr:primaryType="nt:unstructured"/>
            </content>
        </components>
    </actingreel>
</jcr:root>
